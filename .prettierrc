{
  "singleAttributePerLine": true,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "printWidth": 80,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    },
    {
      "files": ["*.jsx", "*.js"],
      "options": {
        "parser": "babel"
      }
    }
  ]
}
